    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react": "^4.2.0",
    "less": "^4.1.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.9",
    "vite-plugin-dynamic-import": "^1.5.0"
  }
}
